import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        int T = sc.nextInt();  // Number of test cases
        
        for (int t = 0; t < T; t++) {
            int N = sc.nextInt();  // Size of array
            int[] A = new int[N];
            
            // Read the array and find the minimum
            int min = Integer.MAX_VALUE;
            for (int i = 0; i < N; i++) {
                A[i] = sc.nextInt();
                if (A[i] < min) {
                    min = A[i];
                }
            }
            
            // Count how many elements are greater than the minimum
            int operations = 0;
            for (int i = 0; i < N; i++) {
                if (A[i] > min) {
                    operations++;
                }
            }
            
            System.out.println(operations);
        }

        sc.close();
    }
}
